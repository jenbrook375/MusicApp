<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/gelbe_note"
        android:label="Mood Music"
        android:roundIcon="@mipmap/gelbe_note"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RelaxingActivity"
            android:label="Music for Relaxing"/>
        <activity android:name=".DrivingActivity"
            android:label="Music for Driving"/>
        <activity android:name=".StudyingActivity"
            android:label="Music for Studying">
        </activity>
    </application>

</manifest>